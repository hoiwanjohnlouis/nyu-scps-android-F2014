<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.acar.j.michael.boggle" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".WelcomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.acar.j.michael.boggle.BoardActivity"
            android:label="@string/title_activity_board"
            android:parentActivityName="com.acar.j.michael.boggle.WelcomeActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="com.acar.j.michael.boggle.WelcomeActivity" />
        </activity>
    </application>

</manifest>